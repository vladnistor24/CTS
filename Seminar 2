//OCP

interface Vehicle {
    double calculateValue();
}

class Car implements Vehicle {
    private double v;
    @Override
    public double calculateValue() {
        return 0;
    }
}

class Bike implements Vehicle {
    private double vehicle;
    @Override
    public double calculateValue() {
        return 0;
    }
}


